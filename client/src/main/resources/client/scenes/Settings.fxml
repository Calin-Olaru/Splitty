<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.SettingsCtrl">
    <children>
        <ComboBox fx:id="selectedLanguage" layoutX="13.0" layoutY="85.0" prefWidth="150.0" promptText="English" />
        <ComboBox fx:id="selectedCurrency" layoutX="13.0" layoutY="124.0" prefWidth="150.0" promptText="EUR" />
        <TextField fx:id="publicUsername" layoutX="14.0" layoutY="166.0" text="Cristian Sapunaru #01" />
        <Button fx:id="btnSubmitAll" layoutX="13.0" layoutY="207.0" mnemonicParsing="false" onAction="#clickBtnSubmitAll" text="Submit" />
        <Text fx:id="settingsTitle" layoutX="251.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" />
        <Button fx:id="btnHome" layoutX="537.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickBtnHome" text="Home" />
    </children>
</AnchorPane>
